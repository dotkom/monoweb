{
  "name": "db-migrator",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "dependencies": {
    "@dotkomonline/db": "workspace:*",
    "tsx": "^3.12.0"
  },
  "scripts": {
    "migrate": "tsx ./src/index.ts",
    "migrate:down": "tsx ./src/down.ts",
    "migrate:up": "tsx ./src/up.ts"
  }
}
